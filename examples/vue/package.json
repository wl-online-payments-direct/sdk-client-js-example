{
  "name": "vue-example",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"vite\" \"yarn watch-styles\"",
    "build": "tsc -b && vite build",
    "lint": "eslint",
    "preview": "vite preview",
    "copy-design": "node ./../../scripts/copy-design.js ./design/images",
    "copy-styles": "npx sass ../../shared/design/styles/index.scss:./design/styles/index.css",
    "watch-styles": "yarn copy-styles --watch",
    "postinstall": "yarn copy-design & yarn copy-styles"
  },
  "dependencies": {
    "vue": "^3.5.18",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.1",
    "typescript": "~5.8.3",
    "sass": "^1.93.2",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5"
  }
}
